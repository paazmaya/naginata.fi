{
  "name": "naginata.fi",
  "version": "0.13.0",
  "title": "Naginata in Finland",
  "homepage": "https://github.com/paazmaya/naginata.fi",
  "description": "A web site in Finland for an ancient martial art from Japan",
  "private": "true",
  "main": "build.js",
  "type": "module",
  "subdomain": "naginata-finland",
  "scripts": {
    "test": "jest --logHeapUsage --verbose",
    "css": "postcss src/css/*.css -o public_html/css/naginata.min.css --verbose",
    "js": "terser src/js/*.js --compress --mangle --output public_html/js/naginata.min.js",
    "lint": "eslint *.js tests lib src",
    "hover": "URL=http://localhost:5000/en/naginata node tests/naginata-bogu-hover.js",
    "start": "http-server -p 5000 public_html",
    "build": "node build.js && npm run js && npm run css"
  },
  "dependencies": {
    "marked": "4.0.15",
    "nunjucks": "3.2.3"
  },
  "devDependencies": {
    "autoprefixer": "10.4.7",
    "cssnano": "5.1.7",
    "eslint": "8.28.0",
    "eslint-config-paazmaya": "9.0.0",
    "http-server": "14.1.0",
    "jest": "28.1.0",
    "playwright": "1.21.1",
    "postcss": "8.4.13",
    "postcss-cli": "9.1.0",
    "terser": "5.13.1"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paazmaya/naginata.fi.git"
  },
  "bugs": {
    "url": "https://github.com/paazmaya/naginata.fi/issues"
  },
  "keywords": [
    "atarashii",
    "naginata",
    "jikishin",
    "jikishinkageryu",
    "koryu",
    "finland",
    "budo",
    "martial-art"
  ],
  "browserslist": [
    "last 3 versions"
  ],
  "author": "Juga Paazmaya <paazmaya@yahoo.com> (https://paazmaya.fi)",
  "contributors": [
    {
      "name": "Leena Lecklin"
    },
    {
      "name": "Mari Paasonen"
    }
  ],
  "license": "CC-BY-SA-4.0"
}
